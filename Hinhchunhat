#include <stdio.h>

int main() {
    float width = 5.0;  // Chiều rộng của hình chữ nhật
    float height = 10.0; // Chiều dài của hình chữ nhật
    float area;

    // Tính diện tích hình chữ nhật
    area = width * height;

    // In ra diện tích
    printf("Diện tích hình chữ nhật là: %.2f\n", area);

    return 0;
}
